{
    

    "middleware": {

        "devtools": {
            "enabled": true,
            "priority": 35,
            "module": {
                "name": "construx",
                "arguments": [
                    "path:./public",
                    "path:./.build",
                    {
                        
                        
                        "copier": {
                            "module": "construx-copier",
                            "files": "**/*"
                        }
                    }
                ]
            }
        },
        "cors": {
          "enabled": true,
          "priority": 119,
          "module": {
              "name": "path: ./middlewares/cors"
          }
      }
    }
}
